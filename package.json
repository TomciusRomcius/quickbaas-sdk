{
  "name": "quickbaas-sdk",
  "version": "1.0.0",
  "description": "A javascript sdk for making API calls to QuickBaas",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon --watch src/ --exec pnpm run build -e ts",
